{
    "name": "SolarDB-Manager",
    "version": "2.0.1",
    "description": "GUI de SolarDB",
    "main": "main.js",
    "repository": "https://github.com/gusgeek/SolarDB-Manager",
    "author": "gusgeek",
    "license": "Apache-2.0",
    "bugs": {
      "url": "https://github.com/gusgeek/SolarDB-Manager/issues"
    },
    "homepage": "https://github.com/gusgeek/SolarDB-Manager#readme",
    "scripts": {
      "test": "electron .",
      "test-build": "nelectron-builder -wlm",
      "build": "electron-builder -wlm -p 'always'"
    },
    "devDependencies": {
      "electron": "^15.0.0",
      "electron-builder": "^22.13.1",
      "gh-pages": "^3.2.3"
    },
    "build": {
      "productName": "SolarDB-Manager",
      "appId": "com.gusgeek.SolarDB-Manager",
      "directories": {
        "buildResources": "resources"
      },
      "win": {
        "icon": "resources/iconos/support.png",
        "target": [
          {
            "target": "nsis"
          }
        ]
      },
      "linux": {
        "icon": "resources/iconos/support.png",
        "target": [
          {
            "target": "AppImage"
          }
        ]
      },
      "mac": {
        "icon": "resources/iconos/support.png",
        "target": [
          {
            "target": "DMG"
          }
        ]
      },
      "publish": {
        "provider": "github"
      }
    },
    "dependencies": {
      "axios": "^0.23.0",
      "bson": "^4.5.3",
      "electron-browser-storage": "^1.0.7",
      "electron-log": "^4.4.1",
      "electron-updater": "^4.3.9",
      "jquery": "^3.6.0"
    }
  }
  